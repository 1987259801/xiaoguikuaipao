/* pages/home/search/result/result.wxss */

Page {
  max-height: 100%;
  width: 100%;
  background-color: #e6e6e6;
}
.g-clr:after {
  width: 100%;
  clear: both;
  content: '';
  zoom: 1;
  display: block;
}
.tarfixed {
  position: fixed;
  top: 80rpx;
  left: 0px;
  z-index: 1000;
}

.tar_container {
  display: flex;
  flex-direction: row;
  border-bottom: 1rpx solid #eee;
}

.single_tar {
  display: inline-block;
  width: 750rpx;
  height: 105rpx;
  background: white;
}

.tar_name {
  height: 100rpx;
  width: 100%;
  font-size: 30rpx;
  text-align: center;
  color: #444;
  line-height: 100rpx;
}

.tarSelected {
  height: 7rpx;
  width: 100%;
  background: #5dd9ab;
}

.textcolor {
  color: #5dd9ab;
}

.clearChoice {
  font-size: 30rpx;
  color: #999;
  margin-left: 20rpx;
  width: 150rpx;
  height: 58rpx;
  line-height: 58rpx;
  text-align: left;
}

.finish {
  width: 150rpx;
  height: 58rpx;
  background: rgba(93, 217, 171, 1);
  border-radius: 6rpx;
  font-size: 30rpx;
  float: right;
  margin-left: 30rpx;
  color: white;
  text-align: center;
  line-height: 58rpx;
  margin-right: 20rpx;
}

.discount1 {
  width: 150rpx;
  height: 58rpx;
  background: rgba(93, 217, 171, 1);
  border: 1rpx solid rgba(93, 217, 171, 1);
  border-radius: 6rpx;
  font-size: 30rpx;
  float: left;
  margin-left: 30rpx;
  color: white;
  text-align: center;
  line-height: 58rpx;
  margin-bottom: 20rpx;
  margin-top: 15rpx;
}

.discount0 {
  width: 150rpx;
  height: 58rpx;
  border: 1rpx solid #bfbfbf;
  border-radius: 6rpx;
  font-size: 30rpx;
  float: left;
  margin-left: 30rpx;
  color: #666;
  text-align: center;
  line-height: 58rpx;
  margin-bottom: 20rpx;
  margin-top: 15rpx;
}

.orderStyle1 {
  color: rgb(93, 217, 171);
  background-color: #e6e6e6;
  height: 102rpx;
  width: 100%;
  line-height: 102rpx;
}

.orderStyle0 {
  color: #666;
  height: 102rpx;
  width: 100%;
  line-height: 102rpx;
}

.weui-navbar {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  position: fixed;
  z-index: 500;
  top: 76rpx;
  width: 100%;
  border-bottom: 1rpx solid white;
}

.weui-tab {
  position: fixed;
  top: 76rpx;
  height: 100%;
  z-index: 1000;
}

.weui-tab__content {
  padding-top: 60px;
  text-align: center;
}

.weui-navbar__item.weui-bar__item_on {
  color: #5dd9ab;
}

.hidden_class {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  display: inline-block;
}

.input_class {
  display: inline-block;
  background-color: white;
  padding-left:7.3333vw;
  width: 62.6667vw;
  height: 8.8vw;
  font-size:3.7333vw;
  border-radius: 33rpx;
  margin-left: 10.6667vw;
  line-height: 8.8vw;
}

.weui-cell {
  height: 34rpx;
  padding: 0px 15px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.myweui-cell {
  padding: 20rpx 15rpx;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

.addr {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-left: 18rpx;
  display: inline-block;
  color: white;
  font-size: 30rpx;
  text-align: left;
}

.label1 {
  font-family: Jkaton;
  padding: 3rpx 3rpx;
  text-align: center;
  line-height: 28rpx;
  background: #f94400;
  color: white;
  font-size: 20rpx;
  display: index-block;
  height: 28rpx;
  width: 28rpx;
  border-radius: 4rpx;
}

.label2 {
  display: inline-block;
  width: 350rpx;
  font-size: 22rpx;
  font-family: PingFang-SC-Medium;
  color: rgba(153, 153, 153, 1);
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin-left: 9rpx;
  margin-bottom: -6rpx;
}

.shangjai_free {
  display: inline-block;
  width: 100rpx;
  height: 30rpx;
  background: white;
  border-radius: 5rpx;
  font-size: 20rpx;
  color: #f94400;
  text-align: center;
  line-height: 30rpx;
  border: 1rpx solid #f94400;
  margin-left: 10rpx;
}

.shangjia_post {
  display: inline-block;
  width: 88rpx;
  height: 32rpx;
  background: rgba(249, 68, 0, 1);
  border-radius: 16rpx 0px 16rpx 0px;
  font-size: 20rpx;
  color: white;
  text-align: center;
  line-height: 30rpx;
}
.press-style{
  background: #ECECEC;
}