.orderLis-itmes .ul{width:89.333vw; margin: 0 auto; font-size: 3.2vw; padding:2vw 0;  }
.orderLis-itmes .ul .li{min-height:9.2vw; line-height: 9.2vw; color:#777777;font-size: 3.8vw;text-overflow: ellipsis;white-space: nowrap;overflow: hidden;}
.orderLis-itmes .ul .li image{width: 6vw; height:6vw; vertical-align: middle; margin:0 3vw 0 0; margin-top: -1vw;}