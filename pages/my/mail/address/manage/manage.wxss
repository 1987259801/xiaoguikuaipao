  z-index: 999;
}

.manage-content_duplication {
  clear: both;
  margin-bottom: 20rpx;
}

.manage-content_compellation {
  color: #999;
  float: left;
  margin-top: 0.75vh;
  margin-right: 2.6vw;
}

.manage-content_telephone {
  float: left;
  margin-top: 0.55vh;
  color: #999;
}

.manage-content_duplication1 {
  margin-left: 4vw;
}

.manage-content_textarea {
  width: 89.3333vw;
  margin-top: 4vw;
  margin-bottom: 2.5333vw;
  color: #000;
  line-height: 4.5vw;
}

checkbox {
  border-radius: 50vw;
}

.is_checked {
  width: 34rpx;
  height: 34rpx;
  border: 1px solid #9c9c9c;
  border-radius: 50%;
  margin-left: 4vw;
  float: left;
  margin-right: 2.8vw;
}

.normal {
  width: 34rpx;
  height: 34rpx;
  border: 1px solid white;
  background: url(https://qnimg.xiaoguikuaipao.com/Fm-4yvBdoKYgh48SYn5mf7GhGUqA) no-repeat;
  background-size: 100%;
  margin-left: 4vw;
  float: left;
  margin-right: 2.8vw;
}

.manage-content_site {
  font-size: 3vw;
  color: #666;
  float: left;
  margin-top: 0.3vh;
}

.management {
  background: #5dd9ab;
  color: #fdf7f7;
  width: 93.8vw;
  position: fixed;
  font-size: 3.4vw;
  line-height: 6vh;
  z-index: 999;
}

.manage-up_image1 {
