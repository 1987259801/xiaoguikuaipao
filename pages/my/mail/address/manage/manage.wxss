Page {
  background: rgba(255, 255, 255, 1);
  font-family: "FZLTHJW--GB1-0";
  font-size: 3.6vw;
}

.xian {
  border: 1vh solid #f8f8f8;
}

.xian1 {
  border-bottom: 1px solid rgba(238, 238, 238, 1);
  margin-top: 3.3vw;
  margin-bottom: 2.6666vw;
}

.manage-up {
  background: rgba(255, 255, 255, 1);
  height: 6vh;
  width: 100vw;
  padding-top: 1.155vh;
  position: fixed;
  top: 0px;
  z-index: 999;
}

.manage-up_search {
  font-size: 3vw;
  border-radius: 20vw;
  width: 92.5vw;
  height: 5vh;
  margin-left: 2.2vw;
  padding-left: 2.5vw;
}

.reloading {
  margin-left: 4.5vw;
}

.manage-up_image {
  width: 2.95vw;
  height: 2vh;
  margin-top: 1.5vh;
  margin-left: 4.6vw;
