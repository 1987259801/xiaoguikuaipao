}
.order_distribution_content .btn_startup::after{border: none;}
.order_distribution_content .btn_startup image{width: 22rpx;height: 13rpx;position: relative;bottom: 4rpx;padding-left: 4rpx;}
/* 头部 */
.distribution_box{}
.distribution_box .unpay_top{display: flex;flex-flow: column;justify-content: center;text-align: center;background: #fff;margin: 20rpx;border-radius: 20rpx;}
.distribution_box .unpay_top .unpay_top_h{font-size: 35rpx;font-weight: bold;padding: 30rpx 0rpx 10rpx;}
.distribution_box .unpay_top .unpay_top_c{font-size: 30rpx;color: #444;}
.distribution_box .unpay_top .unpay_top_c image{width: 27rpx;height: 27rpx;padding-right: 10rpx;}
.distribution_box .unpay_top .txt_timebg1{color: #5DD9AB;}
.distribution_box .unpay_top .txt_timebg2{color: rgb(0, 145, 220);}
.distribution_box .unpay_top .unpay_top_b{padding-top: 30rpx;display: block;padding-bottom: 20rpx;}
.distribution_box .unpay_top .unpay_top_b button{height: 60rpx;width: 180rpx;font-size: 27rpx;line-height: 60rpx;display: inline-block;}
.distribution_box .unpay_top .unpay_top_b button.btn_cancel{background: #fff;border-color: #999;}
.distribution_box .unpay_top .unpay_top_b button.btn_gopay{background: #5DD9AB;margin-left: 20rpx;border-color: #5DD9AB;}
.distribution_box .unpay_top .unpay_top_b button.btn_gopay::after{border: none;}
.border-radius30{border-radius: 30rpx;}

