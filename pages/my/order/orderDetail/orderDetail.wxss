.order_distribution_content .order_pro_price{color: rgb(68, 68, 68);font-size: 4vw;padding-right:10rpx;}
.order_distribution_content .order_total{text-align: center;padding: 20rpx 0rpx 30rpx;font-size: 30rpx;color:rgb(68 ,68, 68);}
/* .order_distribution_content .order_total .txt1{font-size: 55rpx;color:#E5004F;padding-right: 10rpx;} */
.order_distribution_content .order_total .txt1{font-size: 6.6667vw;color:#444;padding-right: 10rpx;}
.order_distribution_content .order_total .txt3{color: #444;}
.order_distribution_content .order_total .txt2{color:rgb(153, 153, 153);}
